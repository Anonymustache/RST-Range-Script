local uis = game:GetService("UserInputService")
local db=false
local dh = false
local ScreenGui = Instance.new("ScreenGui")
local Frame = Instance.new("Frame")
local Pattern = Instance.new("ImageLabel")
local TextLabel = Instance.new("TextLabel")
local name = Instance.new("TextLabel")
local instruction1 = Instance.new("TextLabel")
local instruction2 = Instance.new("TextLabel")

--Properties:

ScreenGui.Parent = game.CoreGui
ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

Frame.Parent = ScreenGui
Frame.BackgroundColor3 = Color3.fromRGB(99, 95, 98)
Frame.BorderColor3 = Color3.fromRGB(79, 76, 78)
Frame.BorderSizePixel = 2
Frame.Position = UDim2.new(.5,0,.5,0)
Frame.Size = UDim2.new(0.001,0,0.001,0)

Pattern.Name = "Pattern"
Pattern.Parent = Frame
Pattern.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
Pattern.BackgroundTransparency = 1.000
Pattern.Size = UDim2.new(1, 0, 1, 0)
Pattern.ZIndex = 2
Pattern.Image = "rbxassetid://10530276"
Pattern.ImageTransparency = 0.600
Pattern.ScaleType = Enum.ScaleType.Tile
Pattern.SliceCenter = Rect.new(0, 256, 0, 256)
Pattern.TileSize = UDim2.new(0, 30, 0, 30)

TextLabel.Parent = Frame
TextLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
TextLabel.BackgroundTransparency = 1.000
TextLabel.Size = UDim2.new(1, 0, 0.296000004, 0)
TextLabel.ZIndex = 3

-- Introduction:

Frame:TweenSizeAndPosition(UDim2.new(0.221, 0,0.306, 0), UDim2.new(0.389, 0,0.347, 0), Enum.EasingDirection.Out, Enum.EasingStyle.Quad, 2)
wait(5)
Frame:TweenSizeAndPosition(UDim2.new(0.001, 0,0.001, 0), UDim2.new(0.5, 0,0.5, 0), Enum.EasingDirection.In, Enum.EasingStyle.Quad, 2)
wait(2)
ScreenGui:Destroy()

-- Scripts:

local function findTool(name)
	if game.Players.LocalPlayer.Backpack:FindFirstChild(name) then
		return game.Players.LocalPlayer.Backpack[name].HitPart
	elseif game.Players.LocalPlayer.Character:FindFirstChild(name) then
		return game.Players.LocalPlayer.Character[name].HitPart
	else
		print('no tool')
	end
end
local function findTTool(name)
	if game.Players.LocalPlayer.Backpack:FindFirstChild(name) then
		return game.Players.LocalPlayer.Backpack[name].HitPart
	elseif game.Players.LocalPlayer.Character:FindFirstChild(name) then
		return game.Players.LocalPlayer.Character[name].HitPart
	else
		print('no tool')
	end
end
local function box(item)
	pcall(function()
		local a = Instance.new("SelectionBox", item)
		a.Adornee = item
	end)
end
local function unbox(item)
	pcall(function()
		item.SelectionBox:Destroy()
	end)
end

uis.InputBegan:Connect(function(key,c)
	if c then return end

	if key.KeyCode == Enum.KeyCode.Delete then
		if not db then
			db = true
			pcall(function()
				local p1 = findTool("Punch")
				local p2 = findTool("Haymaker")
				local p3 = findTool("Uppercut")
				local p4 = findTool("Kick")
				local p5 = findTTool("Punch")
				local p6 = findTTool("Haymaker")
				local p7 = findTTool("Uppercut")
				local p8 = findTTool("Kick")

				p1.Size = Vector3.new(3, 2, 1.1)
				p2.Size = Vector3.new(3, 2, 1.1)
				p3.Size = Vector3.new(3, 2, 1.1)
				p4.Size = Vector3.new(3, 2, 1.1)

				p1.Maseless = true
				p2.Maseless = true
				p3.Maseless = true
				p4.Maseless = true

				p5.Size = Vector3.new(3, 2, 1.1)
				p6.Size = Vector3.new(3, 2, 1.1)
				p7.Size = Vector3.new(3, 2, 1.1)
				p8.Size = Vector3.new(3, 2, 1.1)

				p5.Maseless = true
				p6.Maseless = true
				p7.Maseless = true
				p8.Maseless = true
			end)
		else
			pcall(function()
				db = false
				local p1 = findTool("Punch")
				local p2 = findTool("Haymaker")
				local p3 = findTool("Uppercut")
				local p4 = findTool("Kick")

				p1.Size = Vector3.new(1, 2, 1)
				p2.Size = Vector3.new(1, 2, 1)
				p3.Size = Vector3.new(1, 2, 1)
				p4.Size = Vector3.new(1, 3, 1)

				p1.Maseless = false
				p2.Maseless = false
				p3.Maseless = false
				p4.Maseless = false
			end)
		end
	elseif key.KeyCode == Enum.KeyCode.End then
		if not dh then
			dh=true
			box(findTool("Punch"))
			box(findTool("Haymaker"))
			box(findTool("Kick"))
			box(findTool("Uppercut"))
		else
			dh=false
			unbox(findTool("Punch"))
			unbox(findTool("Haymaker"))
			unbox(findTool("Kick"))
			unbox(findTool("Uppercut"))
		end
	end
end)

game.Players.LocalPlayer.CharacterAdded:Connect(function(char)
	if db then
		pcall(function()
			local p1 = findTool("Punch")
			local p2 = findTool("Haymaker")
			local p3 = findTool("Uppercut")
			local p4 = findTool("Kick")
			local p5 = findTTool("Punch")
			local p6 = findTTool("Haymaker")
			local p7 = findTTool("Uppercut")
			local p8 = findTTool("Kick")

			p1.Size = Vector3.new(3, 2, 1.1)
			p2.Size = Vector3.new(3, 2, 1.1)
			p3.Size = Vector3.new(3, 2, 1.1)
			p4.Size = Vector3.new(3, 2, 1.1)

			p1.Maseless = true
			p2.Maseless = true
			p3.Maseless = true
			p4.Maseless = true

			p5.Size = Vector3.new(3, 2, 1.1)
			p6.Size = Vector3.new(3, 2, 1.1)
			p7.Size = Vector3.new(3, 2, 1.1)
			p8.Size = Vector3.new(3, 2, 1.1)

			p5.Maseless = true
			p6.Maseless = true
			p7.Maseless = true
			p8.Maseless = true
		end)
	end
	if dh then
		box(findTool("Punch"))
		box(findTool("Haymaker"))
		box(findTool("Kick"))
		box(findTool("Uppercut"))
	end
end)
if game.Players.LocalPlayer:IsInGroup(4634159) then
	game.Players.LocalPlayer:Kick('no fat')
end
